{
  "version": 2,
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "rewrites": [
    { 
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/v1/attorneys",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate=59"
        }
      ]
    }
  ]
} 