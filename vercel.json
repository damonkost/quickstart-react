{
  "version": 2,
  "functions": {
    "pages/api/v1/**/*.js": {
      "runtime": "@vercel/node@latest"
    }
  },
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "/api/v1/:path*"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/v1/(.*)",
      "dest": "/api/v1/$1",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ]
} 