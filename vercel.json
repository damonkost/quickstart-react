{
  "version": 2,
  "framework": "nextjs",
  "routes": [
    {
      "src": "/api/v1/attorneys",
      "methods": ["GET"],
      "dest": "/api/v1/attorneys"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        }
      ]
    }
  ]
} 